        </StackPanel>

        <!--ContentPanel - place additional content here-->
        <Grid x:Name="ContentPanel" Grid.Row="1" Margin="12,0,12,0">
            <Button Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Height="167" Width="161" Margin="6,-42,0,0" Background="#FF0008A8">
                <i:Interaction.Triggers>
                    <i:EventTrigger EventName="Click">
                        <ec:NavigateToPageAction TargetPage="/all_navigate.xaml"/>
