            <TextBlock Text="Kontrast" Style="{StaticResource PhoneTextNormalStyle}"/>
            <TextBlock Text="3 x 4" Margin="9,-7,0,0" Style="{StaticResource PhoneTextTitle1Style}" Height="88"/>
